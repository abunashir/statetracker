<h2>CSV Uploads</h2>

<table class="table table-bordered table-striped top10">
  <tr>
    <th>File Name</th>
    <th>Uploaded At</th>
    <th>Status</th>
  </tr>

  <tbody>
    <% @csv_uploads.each do |csv_upload| %>
      <tr>
        <td><%= csv_upload.csv_file.file.filename %></td>
        <td><%= csv_upload.created_at.to_formatted_s(:long) %></td>
        <td><%= csv_upload.status.humanize %></td>
      </tr>
    <%  end %>
  </tbody>
</table>


<br />
<p>
  <%= link_to("Upload A CSV", new_csv_upload_path, class: "btn btn-primary") %>
  <%= link_to("Track an object", new_state_tracker_path, class: "btn btn-info") %>
</p>
